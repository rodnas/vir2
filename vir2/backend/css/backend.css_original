.baseFont {font-size: 12px;text-decoration: none;}
.listHeaderColor {background-color: #BBBBBB;}
.listLineColor1 {background-color: #DDDDDD;min-height:28px;}
.listLineColor1:hover {background-color:#cccccc !important;color:#000000 !important;min-height:28px;}
.listLineColor1 .itemViewRight {float:right;text-align:right;margin-right:5px;font-size: 11px;text-decoration: none;background-color:#cccccc;display:none;min-height:28px;}
.listLineColor1:hover .itemViewRight {float:right;text-align:right;margin-right:5px;font-size: 11px;text-decoration: none;background-color:#cccccc;display:inline;vertical-align:top;min-height:28px;}
.listLineColor2 {background-color: #EEEEEE;min-height:28px;}
.listLineColor2:hover {background-color:#cccccc !important;color:#000000 !important;min-height:28px;}
.listLineColor2 .itemViewRight {float:right;text-align:right;margin-right:5px;font-size: 11px;text-decoration: none;background-color:#cccccc;display:none;min-height:28px;}
.listLineColor2:hover .itemViewRight {float:right;text-align:right;margin-right:5px;font-size: 11px;text-decoration: none;background-color:#cccccc;display:inline;vertical-align:top;min-height:28px;}
.titleColor {background-color: #DDDDDD;}
.fieldColor {background-color: #F3F2eb;}
#itemTitle {float:left;padding:2px;text-align:left;font-size: 11px;text-decoration: none;background-color:#DDDDDD;margin-left:5px;border-radius:5px;}
#searchFilterTitle {float:left;padding:3px;text-align:center;font-size: 12px;text-decoration: none;color:{##actualFgColor##};font-weight:bold;margin-left:5px}
#itemView {float:left;text-align:left;margin-left:5px;font-size: 11px;text-decoration: none;}
#thumbView {width:120px;border:0;}
#itemViewRight {float:right;text-align:right;margin-right:5px;font-size: 11px;text-decoration: none;}
#itemViewDescription {text-align:left;font-size: 11px;text-decoration: none;background:white;border:1px solid #000000;padding:5px;width:98%;margin:auto;border-radius:5px;}
#itemLineTitle {padding:2px;text-align:center;font-size: 11px;text-decoration: none;background-color:#DDDDDD;border-radius:5px;}
#listHeader {width:98%;padding-top:5px;-color:#bbbbbb;margin-left:5px;margin:auto;}
#listItem {width:98%;padding-top:5px;margin-left:5px;margin-right:5px;border-radius:5px;margin:auto;min-height:30px;}
form {padding:0;margin:0;}

#addFrame {flost:center;position:absolute; margin-left:20px; margin-top:5px; text-align:center;background-color:#cccccc;border:solid 1px;border-radius:5px;}
#popupCode {width:800px;text-align:center;background-color:#F5F5F5;border:solid 1px;border-radius:5px;}
#runtime {margin-right:10px;font-size:11px;color:{##actualFgColor##};float:right;}
#button24x24{float:left;font-size:20px;font-weight:bold;border:2px solid {##actualFgColor##};border-radius:6px;border-style:solid;height:24px;width:24px;background:{##actualBgColor##};color:{##actualFgColor##};line-height:24px;text-align:center;}
#button24x24:hover {float:left;font-size:20px;font-weight:bold;border:2px solid {##actualFgColor##};border-radius:6px;border-style:solid;height:24px;width:24px;background:{##actualBgColor##};color:{##actualFgColor##};line-height:24px;text-align:center;}
#contentFrameMiniHead {float:left;color:{##actualFgColor##};margin-left:5px;margin-top:10px;position:absolute;}
#contentLeft {float:left;border-radius:10px;border-style:solid;border-width:2px;border-color:{##actualFgColor##};width:560px;}
#contentRight {float:right;border-radius:10px;border-style:solid;border-width:2px;border-color:{##actualFgColor##};width:710px;}